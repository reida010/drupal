{
    "name": "reida010/drupal",
    "description": "Mesh.lu project template for Drupal 9 projects with Composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "David Reichling",
            "email": "david@mesh.lu"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.3",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^2.4",
        "drupal/autofill": "^1.0",
        "drupal/console": "^1.9",
        "drupal/core-composer-scaffold": "^9.0",
        "drupal/core-recommended": "^9.0",
        "drupal/crop": "^2.1",
        "drupal/ctools": "^3.4",
        "drupal/diff": "^1.0",
        "drupal/dropzonejs": "^2.3",
        "drupal/empty_fields": "^1.0@beta",
        "drupal/entity_browser": "^2.5",
        "drupal/focal_point": "^1.5",
        "drupal/gin": "^3.0@alpha",
        "drupal/gin_toolbar": "^1.0@beta",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/length_indicator": "^1.1",
        "drupal/linkit": "^4.3",
        "drupal/media_entity_instagram": "^3.0",
        "drupal/media_entity_slideshow": "^2.0",
        "drupal/media_entity_twitter": "^2.4",
        "drupal/metatag": "^1.14",
        "drupal/paragraphs": "^1.12",
        "drupal/paragraphs_features": "^1.11",
        "drupal/password_policy": "^3.0@beta",
        "drupal/pathauto": "^1.8",
        "drupal/redirect": "^1.6",
        "drupal/select2": "^1.12",
        "drupal/shariff": "^1.7",
        "drupal/token": "^1.7",
        "drush/drush": "^10.3",
        "npm-asset/dropzone": "^5.7",
        "npm-asset/select2": "^4.0",
        "npm-asset/shariff": "^3.2",
        "vlucas/phpdotenv": "^5.1",
        "webflo/drupal-finder": "^1.2"
    },
    "require-dev": {
        "drupal/core-dev": "^9.0",
        "drupal/devel": "^4.0",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
        }
    }
}
